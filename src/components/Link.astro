---
interface Props {
  title: string;
  url: string;
  target: string;
}

const { title, url, target } = Astro.props;
---

<a
  class="text-sm text-l/tertiary outline-none hover:text-l/primary focus:text-l/primary focus-visible:text-l/primary active:text-l/primary dark:text-d/tertiary dark:hover:text-d/primary dark:focus:text-d/primary dark:focus-visible:text-d/primary dark:active:text-d/primary"
  href={url}
  target={target}>{title}</a
>
